{
  "name": "@mml-playground/server",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": "20.x"
  },
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "iterate": "concurrently \"tsx build.ts --watch\" \"nodemon\"",
    "build": "rimraf ./build && tsx build.ts --build",
    "start": "NODE_ENV=production node build/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,}\" --max-warnings 0",
    "lint:fix": "eslint \"./src/**/*.{ts,}\" --fix"
  },
  "dependencies": {
    "@mml-io/3d-web-experience-server": "0.16.0",
    "networked-dom-server": "0.14.0",
    "canvas": "2.11.2",
    "web-client": "file:../web-client",
    "chokidar": "3.6.0",
    "express": "4.19.2",
    "express-ws": "5.0.2",
    "cors": "2.8.5",
    "ws": "8.17.0",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17",
    "@types/jsdom": "21.1.6",
    "@types/express-ws": "3.0.4",
    "@types/http-proxy": "1.17.14",
    "@types/node": "20.12.12",
    "nodemon": "3.1.0"
  }
}
