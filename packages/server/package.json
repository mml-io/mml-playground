{
  "name": "@mml-playground/server",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": "18.19.0"
  },
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "iterate": "concurrently \"tsx build.ts --watch\" \"nodemon\"",
    "build": "rimraf ./build && tsx build.ts --build",
    "start": "NODE_ENV=production node build/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,}\" --max-warnings 0",
    "lint:fix": "eslint \"./src/**/*.{ts,}\" --fix"
  },
  "dependencies": {
    "@mml-io/3d-web-text-chat": "0.13.0",
    "@mml-io/3d-web-user-networking": "0.13.0",
    "@dolbyio/dolbyio-rest-apis-client": "4.1.0",
    "canvas": "2.11.2",
    "web-client": "file:../web-client",
    "networked-dom-server": "0.11.3",
    "chokidar": "3.5.3",
    "express": "4.18.2",
    "express-ws": "5.0.2",
    "cors": "2.8.5",
    "http-proxy": "1.18.1",
    "ws": "8.16.0",
    "dotenv": "16.4.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17",
    "@types/jsdom": "21.1.6",
    "@types/express-ws": "3.0.4",
    "@types/http-proxy": "1.17.14",
    "@types/node": "20.11.15",
    "nodemon": "3.0.3"
  }
}
